package com.rubypaper.controller;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class SecondController {
	//요청패스
	@GetMapping("/hi")
	public String hi() {
		return "hi!!!~~~";
	}
	
	@GetMapping("/yuhan")
	public String yuhan() {
		return "yuhan 화이팅!!";
	}
	
}
